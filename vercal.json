{
    "builds": [{
        "src": "resume_app/wsgi.py",
        "use": "@vercal/python",
        "config": { "maxLambdaSize": "15mb" , "runtime":"python3.9" }
    },
    
    {
        "src": "build_files.sh",
        "use": "@vercal/static-build ",
        "config": { "distDir":"staticfiles_build"}
    }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "resume_app/wsgi.py"
        }
    ] 
}